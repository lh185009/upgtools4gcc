$ git ls-tree -r --name-only HEAD | awk '
{
  split($0, parts, "/");
  path = "";
  for (i = 1; i <= length(parts); i++) {
    path = (i == 1) ? parts[i] : path "/" parts[i];
    if (!seen[path]) {
      indent = "";
      for (j = 1; j < i; j++) indent = indent "│   ";
      prefix = (i == length(parts)) ? "├── " : "";
      print indent prefix parts[i];
      seen[path] = 1;
    }
  }
}'
